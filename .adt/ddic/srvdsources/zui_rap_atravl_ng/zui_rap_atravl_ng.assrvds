define service ZUI_RAP_ATRAVL_NG {
  expose ZC_RAP_ATRAVL_NG  as Travel;
  expose ZC_RAP_BOOKING_NG as Booking;
  expose /DMO/I_Agency     as Agency;
  expose /DMO/I_Customer   as Customer;
  expose /DMO/I_Flight     as Flight;
